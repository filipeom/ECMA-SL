function main() {
	x: {} := {};																										/* Valid */
	y: { foo: number } := { foo: 10 };															/* Valid */
	z: { foo: number, bar: string } := { foo: 10, bar: "abc" };			/* Valid */

	x: { foo: number } := { foo: 10, foo: 20 };											/* DuplicatedField: foo */
	z: { foo: number } := { foo: 10, bar: "abc" };									/* ExtraField: bar */
	x: { foo: number, bar: string } := { foo: 10 };									/* MissingField: bar */
	w: { foo: number, bar: string } := { foo: 10, bar: true };			/* BadField(bar): string <- boolean */
	
	obj: { foo: number, bar: string } := { foo: 10, bar: "abc" };
	x: { foo: number, bar: string, baz: boolean } := obj;						/* MissingField: baz */
	y: { foo: number, bar: boolean } := obj;												/* BadField(bar): boolean <- string */
	return 0
}