function main() {
	obj_simple: { foo: number, bar: string } := { foo: 10, bar: "abc" };
	obj_complex: { foo: { bar: string } } := { foo: { bar: "abc" } };
	obj_complex_undef: { foo: undefined } := { foo: 'undefined };
	obj_undef: undefined := 'undefined;
	obj_null: null := null;
	obj_unknown: unknown := 10;
	obj_false: number := 10;

	obj_simple.foo := 10;									/* Valid */
	obj_simple.bar := "abc";							/* Valid */
	obj_complex.foo.bar := "abc";					/* Valid */
	
	obj_simple.bar := 10;									/* BadValue: string <- number */
	obj_simple.baz := 10;									/* BadLookup: obj.baz */
	obj_complex.foo.bar := 10;						/* BadValue: string <- number */

	obj_complex_undef.foo.bar := "abc";		/* BadPossibleType: obj_complex_undef.foo <- undefined */
	obj_undef.foo := 10;									/* BadPossibleType: undef_obj <- undefined */
	obj_null.foo := 10;										/* BadPossibleType: null_obj <- null */
	obj_unknown.foo := 10;								/* BadType: unknown_obj <- unknown */
	obj_false.foo := 10;									/* BadLookup: false_obj.foo */
	return 0
}