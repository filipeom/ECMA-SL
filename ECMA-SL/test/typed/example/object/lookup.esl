function main() {
	obj_simple: { foo: number, bar: string } := { foo: 10, bar: "abc" };
	obj_complex: { foo: { bar: string } } := { foo: { bar: "abc" } };
	obj_complex_undef: { foo: undefined } := { foo: 'undefined };
	obj_undef: undefined := 'undefined;
	obj_null: null := null;
	obj_unknown: unknown := 10;
	obj_false: number := 10;

	x: number := obj_simple.foo;				/* Valid */
	y: string := obj_simple.bar;				/* Valid */
	z: string := obj_complex.foo.bar;		/* Valid */
	
	x: number := obj_simple.bar;				/* BadValue: number <- string */
	y: number := obj_simple.baz;				/* BadLookup: obj.baz */
	z: number := obj_complex.foo.bar;		/* BadLookup: number <- string */

	obj_complex_undef.foo.bar;		/* BadPossibleType: obj_complex_undef.foo <- undefined */
	obj_undef.foo;								/* BadPossibleType: undef_obj <- undefined */
	obj_null.foo;									/* BadPossibleType: null_obj <- null */
	obj_unknown.foo;							/* BadType: unknown_obj <- unknown */
	obj_false.foo;								/* BadLookup: false_obj.foo */
	return 0
}