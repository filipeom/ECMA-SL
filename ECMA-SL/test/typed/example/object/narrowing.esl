function main() {
	objSimple: { foo: number | string } := { foo: 10 };
	objComplex: { foo: { bar: number | string } } := { foo: { bar : 10 } };
	objSpecial: { foo: any, bar: unknown } := { foo: 10, bar: "abc" };

	x: number | string := objSimple.foo;				/* Valid */
	y: number | string := objComplex.foo.bar;		/* Valid */
	z: number := objSimple.foo;									/* BadValue: number <- string */
	w: string := objComplex.foo.bar;						/* BadValue: string <- number */

	objSimple.foo := "abc";
	objComplex.foo.bar := "abc";
	x: string := objSimple.foo;					/* BadValue: string <- number */
	y: string := objComplex.foo.bar;		/* BadValue: string <- number */

	x: number := objSpecial.foo;				/* Valid */
	y: string := objSpecial.foo;				/* Valid */
	z: string := objSpecial.bar;				/* BadValue: string <- unknown */
	return 0
}