function main() {
  if (true) {
    foo: number := 10;
    bar: number := 10;
    baz: number := 10;
    qux: number := 10;
    fred: any := 10
  } else {
    foo: number := 10;
    bar: string := "abc";
    baz: any := true
    /* qux does not exist in this branch */
    /* fred does not exist in this branch */
  };

  x: number := foo;             /* foo: number */
  y: number | string := bar;    /* bar: number | string */
  z: string := baz;             /* baz: any */
