import "../implementation/ES5_interpreter/section 15/section_15.4.esl";

/* TESTING FUNCTIONS */

/* array constructor length */
function testArrayConstructorLen(global) {
  arr := ArrayConstructorLen(global, 'null, [5.]);

  refLen := newPropertyReference(arr, "length", false);
  @GetValue(len, refLen);

  print "Asserting len";
  assert (len = 5.);
  return null
};

function runTests(global, tests) {
  tests_len := l_len tests;
  i := 0;
  while (i < tests_len) {
    test_name := l_nth(tests, i);
    {test_name}(global);
    i := i + 1
  };
  return null
};

function main() {
  globalObject := NewObject(); /* TODO global object (15.1) */
  setAllInternalMethodsOfObject(globalObject);

  initArrayLib(globalObject, 'null, false);
  print "Create array lib successfully";
  tests := ["testArrayConstructorLen"];
  runTests(globalObject, tests);

  return null
}



