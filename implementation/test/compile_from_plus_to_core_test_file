function main() {
  x := {
    a: 1,
    b: "two"
  };

  x["c"] := {
    c: true
  };

  x["d"] := 1.2;

  y := func("a", 3);

  z := f(g(h(3)));

  i := 0;
  repeat { i := i + 1 } until i <= 4;

  match x.c with
  | { type: "var", vname: x } -> { z(2) }
  | { prop: w, vname: None } -> { z(3) }
  | { prop: w, vname: y } -> { y+w }
  | default -> { z(4) }
}
